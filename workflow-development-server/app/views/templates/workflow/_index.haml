.tab-content
  %md-card.workflows
    %h3 Workflows
    %md-virtual-repeat-container
      .workflow{ md_virtual_repeat: 'wf in pageCtrl.workflows', ng_click: 'appCtrl.tabManagement.openPage("Workflow", wf.id)' }
        %span {{wf.name}}
        %span {{wf.id}}

.tab-top-bar
  = render partial: 'templates/shared/top_bar'
